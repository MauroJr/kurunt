var fs = require('fs')
  , read = fs.readFileSync;

// images

var image = read(__dirname + '/one.jpeg');


  console.log('sending %dkb image', image.length / 1024 | 0);
  
  
  console.log(image);
 
 
 // sock.send(img);


	 var path = '/tmp/' + (Math.random() * 0xffffff | 0) + '.png';
	fs.writeFile(path, image, function(err){
		if (err) throw err;
		console.log('saved %s', path);
	});
	
