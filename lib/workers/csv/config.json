{
	"name": "csv",
	"title": "CSV",	
	"description": "Comma Separated Values, for processing tuple data.",
	"icon": "",
	"url": "http://docs.kurunt.com/CSV_Worker",
	"version": 0.2,	
	"date_mod": "10/22/2013",
	"inputs": [ "tcp", "udp", "http" ],
	"mq_nodelay": false,
	"reports": [ "stream" ],
	"message_codec": "json",
	"encoding": "utf8",
	"stores": [
		{
			"stream": {
				"schema": {
					"tuples": { }
				}
			}
		},
		{
			"mongo": {
				"schema": {
					"tuples": { }
				}
			}
		},
		{
			"redis": {
				"schema": {
					"tuples": { }		
				}
			}
		},
		{
			"mysql": {
				"schema": {
					"tuples": { "type": "varchar(512)" }
				}
			}
		}
	]
}
