{
	"name": "buffer",
	"title": "Buffer",	
	"description": "Buffer, forwards unaltered messages as node.js buffer.",
	"icon": "",
	"url": "http://docs.kurunt.com/Buffer_Worker",
	"version": 0.2,	
	"date_mod": "10/22/2013",
	"inputs": [ "tcp", "udp", "http" ],
	"mq_nodelay": false,
	"reports": [ "stream" ],
	"message_codec": "json",
	"encoding": "utf8",
	"stores": [
		{
			"stream": {
				"schema": {
					"buffer": { }
				}
			}
		},
		{
			"mongo": {
				"schema": {
					"buffer": { }
				}
			}
		},
		{
			"redis": {
				"schema": {
					"buffer": { }
				}
			}
		},
		{
			"mysql": {
				"schema": {
					"buffer": { "type": "varchar(512)" }
				}
			}
		}
	]
}
