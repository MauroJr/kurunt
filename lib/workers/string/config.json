{
	"name": "string",
	"title": "String",	
	"description": "String, for processing text data.",
	"icon": "",
	"url": "http://docs.kurunt.com/String_Worker",
	"version": 0.2,	
	"date_mod": "10/22/2013",
	"inputs": [ "tcp", "udp", "http" ],
	"mq_nodelay": false,
	"reports": [ "stream" ],
	"message_codec": "json",
	"encoding": "utf8",
	"stores": [
		{
			"stream": {
				"schema": {
					"text": { }
				}
			}
		},
		{
			"mongo": {
				"schema": {
					"text": { }
				}
			}
		},
		{
			"redis": {
				"schema": {
					"text": { }		
				}
			}
		},
		{
			"mysql": {
				"schema": {
					"text": { "type": "varchar(512)" }
				}
			}
		}			
	]
}
