{
	"name": "string",
	"title": "String",	
	"description": "String 'worker' for Kurunt, processing text data.",
	"icon": "",
	"url": "http://docs.kurunt.com/worker/string/",
	"version": 0.2,	
	"date_mod": "10/22/2013",
	"inputs": [ "tcp" ],
	"mq_nodelay": false,
	"reports": [ "stream" ],
	"message_codec": "json",
	"encoding": "utf8",
	"stores": [
		{
			"stream": {
				"schema": [
					{
						"name": "text"
					}
				]
			}
		},
		{
			"redis": {
				"schema": [
					{
						"name": "text"
					}
				]
			}
		},		
		{
			"mongo": {
				"schema": [
					{
						"name": "text"
					}
				]
			}
		},		
		{
			"mysql": {
				"schema": [
					{
						"name": "text",
						"type": "varchar(250)"
					},
					{
						"name": "fooInt",
						"type": "tinyint"
					}					
				]
			}
		}
	]
}
